{
  "manifest_version": 3,
  "name": "IBM Task Mining",
  "version": "1.0.0.0",
  "description": "Support extension for IBM Task Mining",
  "permissions": [
    "tabs",
    "nativeMessaging",
    "<all_urls>",
    "webNavigation",
    "downloads"
  ],
  "host_permissions": [
  ],
  "optional_host_permissions": [
    "file:///*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": {
    "script-src": "self unsafe-eval",
    "object-src": "self"
  },
  "icons": {
      "16": "myInvenio16.png",
      "48": "myInvenio48.png",
      "128": "myInvenio128.png"
    }
  }
