<Window x:Class="OpenRPA.NM.Activities.ScreenshotHighlightView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OpenRPA.NM.Activities"
        mc:Ignorable="d"
        Title="Highlight Screenshot selection" Height="480" Width="854 ">
    <Grid>
        <Image x:Name="HighlightScreen" Source="{Binding HighlightScreen}"/>
    </Grid>
</Window>
